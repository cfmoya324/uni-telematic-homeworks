;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.ejercicio.com. ns.ejercicio.com. (
                              6         ; Serial
                             60         ; Refresh
                             60         ; Retry
                             60         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.
3       IN      PTR     ns.ejercicio.com.
2       IN      PTR     www.ejercicio.com.
